<!DOCTYPE html><$mt:Include module="config"$>
<html lang="ja" itemscope itemtype="http://schema.org/WebPage">
<head>
<meta charset="<$mt:PublishCharset$>">
<title><$mt:PageTitle encode_html="1"$> | <$mt:BlogName encode_html="1"$></title>
<meta name="description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
<meta name="keywords" content="<$mt:GetVar name="metakeywords"$>,<$mt:PageKeywords$>">
<meta name="viewport" content="width=device-width,initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<link rel="shortcut icon" href="<$mt:BlogURL encode_html="1"$>images/favicon.ico">
<link rel="start" href="<$mt:BlogURL encode_html="1"$>">
<link rel="alternate" type="application/atom+xml" title="Recent Entries" href="<$mt:Link template="feed_recent"$>">
<$mt:CanonicalLink$>
<!-- Open Graph Protocol -->
<meta property="og:type" content="article">
<meta property="og:locale" content="<$mt:BlogLanguage setvar="blog_lang"$><mt:If name="blog_lang" eq="ja">ja_JP<mt:Else><$mt:Var name="blog_lang"$></mt:If>">
<meta property="og:title" content="<$mt:PageTitle encode_html="1"$> | <$mt:BlogName encode_html="1"$>">
<meta property="og:url" content="<$mt:PagePermalink encode_html="1"$>">
<meta property="og:description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
<meta property="og:site_name" content="<$mt:BlogName encode_html="1"$>">
<meta property="og:image" content="<$mt:BlogURL encode_html="1"$>images/ogimage.png">
<!-- Microdata -->
<meta itemprop="description" content="<$mt:PageExcerpt remove_html="1" encode_html="1"$>">
<link itemprop="url" href="<$mt:PagePermalink encode_html="1"$>">
<link itemprop="image" href="<$mt:BlogURL encode_html="1"$>images/ogimage.png">
<link rel="stylesheet" href="<$mt:BlogRelativeURL$>css/reset.css">
<link rel="stylesheet" href="<$mt:BlogRelativeURL$>style.css">
<link rel="stylesheet" href="<$mt:BlogRelativeURL$>css/jquery.bxslider.css">
<!--[if lt IE 9]>
<script src="//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
<script src="//oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
<![endif]--> 
</head>

<body>

<$mt:Include module="header"$>

<div id="content" class="group">
  <main role="main">
    <section class="entry-detail">
      <article>
        <h1 class="entry-title"><$mt:PageTitle$></h1>
        <section class="entry-body">
          <$mt:PageBody$>
          <$mt:PageMore$>
        </section>
      </article>
  </section>
  </main>
  <aside>
<$mt:Include module="links"$>
<$mt:Include module="banners"$>
<$mt:Include module="facebook"$>
<$mt:Include module="twitter"$>
  </aside>
</div><!-- #content -->

<$mt:Include module="footer"$>

<script src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
  
</body>
</html>
